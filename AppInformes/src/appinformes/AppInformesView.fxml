<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appinformes.AppInformesController">
   <children>
      <MenuBar layoutY="2.0" prefHeight="25.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="Informes">
            <items>
              <MenuItem fx:id="listadoFacturas" mnemonicParsing="false" onAction="#menuListadoFacturas" text="Listado Facturas" />
                  <MenuItem fx:id="ventasTotales" mnemonicParsing="false" onAction="#menuVentasTotales" text="Ventas Totales" />
                  <MenuItem fx:id="facturasCliente" mnemonicParsing="false" onAction="#menuFacturasCliente" text="Facturas por Cliente" />
                  <MenuItem fx:id="subListadoFacturas" mnemonicParsing="false" onAction="#menuSubListadoFacturas" text="Subinforme Listado Facturas" />
            </items>
          </Menu>
          <Menu fx:id="botonAbout" mnemonicParsing="false" text="Help">
               <items>
                  <MenuItem fx:id="about" mnemonicParsing="false" onAction="#menuAbout" text="About" />
                  <MenuItem fx:id="salir" mnemonicParsing="false" onAction="#menuSalir" text="Salir" />
               </items></Menu>
        </menus>
      </MenuBar>
   </children>
</AnchorPane>
